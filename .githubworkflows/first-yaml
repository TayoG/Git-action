name:CI1
on:push
    job1:
         name: Fisrt job
         run-on: umbutu-latest
         steps:
              -name: Step one
               uses: Action/Checkout@v2
               -name: Step two
                run: env | sort  
       job2:
           name: Second job
           run-on: windows-latest
           steps:
                -name: Step one
                 uses: Action/Checkout@v2
                -name: Step two
                  run: "Get-ChildItem Env: | Sort-Object Name"
